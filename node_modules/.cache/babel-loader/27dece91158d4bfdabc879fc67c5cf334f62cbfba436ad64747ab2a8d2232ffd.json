{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { nextTick } from 'vue';\nexport default {\n  name: 'ye',\n  data() {\n    return {\n      count: 2,\n      someObject: {},\n      obj: {\n        nested: {\n          count: 99\n        },\n        arr: ['foo', 'bar']\n      },\n      author: {\n        name: 'John Doe',\n        books: ['Vue 2 - Advanced Guide', 'Vue 3 - Basic Guide', 'Vue4 -The Mystery']\n      },\n      firstName: 'John',\n      lastName: 'Doe'\n    };\n  },\n  computed: {\n    // 一个计算属性的 getter\n    publishedBooksMessage() {\n      // this 指向当前组件实例\n      return this.author.books.length > 0 ? 'yes' : 'NO';\n    },\n    fullName: {\n      get() {\n        return this.firstName + ' ' + this.lastName;\n      },\n      set(newValue) {\n        [this.firstName, this.lastName] = newValue.split('');\n      }\n    }\n  },\n  setup() {\n    const objectOfAttrs = {\n      id: 'container',\n      class: 'warpper'\n    };\n    return {\n      name: \"my Ye\",\n      dynamicId: 1,\n      isButtonDisabled: true,\n      objectOfAttrs,\n      url: 'https://router.vuejs.org',\n      container: 92323\n    };\n  },\n  methods: {\n    async increment() {\n      this.count++;\n      await nextTick();\n      // 现在 DOM 已经更新了\n    },\n    mutateDeeply() {\n      // 以下都会按照期望工作\n      this.obj.nested.count++;\n      this.obj.arr.push('baz');\n      this.obj.arr.push('bar');\n    }\n  },\n  mounted() {\n    // `this` 指向当前组件实例\n    console.log(this.count); // => 1\n\n    // 数据属性也可以被更改\n    this.increment();\n    this.mutateDeeply();\n    console.log(this.obj);\n    const newObject = {};\n    this.someObject = newObject;\n    console.log(this.newObject === this.someObject); // false\n  }\n};","map":{"version":3,"names":["nextTick","name","data","count","someObject","obj","nested","arr","author","books","firstName","lastName","computed","publishedBooksMessage","length","fullName","get","set","newValue","split","setup","objectOfAttrs","id","class","dynamicId","isButtonDisabled","url","container","methods","increment","mutateDeeply","push","mounted","console","log","newObject"],"sources":["C:\\Users\\myboo\\source\\repos\\myvue\\src\\view\\ye.vue"],"sourcesContent":["<template>\r\n    <div class=\"hello\">\r\n        <a :href=\"url\"> vue-cli documentation </a>\r\n        <h1>{{ name }}</h1>\r\n        <h1>{{ count }}</h1>\r\n        <h1>{{ obj.arr.length }}</h1>\r\n        <h1>{{ obj.nested.count }}</h1>\r\n    </div>\r\n    <button @click=\"increment\">{{ count }}</button>\r\n    <div v-bind:id=\"dynamicId\">11111111111111</div>\r\n    <div :id=\"`list-${dynamicId}`\">11111111111111</div>\r\n    <button :disabled=\"isButtonDisabled\">按钮</button>\r\n    <div v-bind=\"objectOfAttrs\">objectOfAttrs对象</div>\r\n    <div>objectOfAttrs.id: {{ objectOfAttrs.id }} </div>\r\n    <p>Has published books:</p>\r\n    <span>{{ publishedBooksMessage }}</span>\r\n</template>\r\n\r\n<script>\r\n    import { nextTick } from 'vue'\r\n    export default {\r\n        name: 'ye',\r\n        data() {\r\n            return {\r\n                count: 2,\r\n                someObject: {},\r\n                obj: {\r\n                    nested: { count: 99 },\r\n                    arr: ['foo','bar']\r\n                },\r\n                author: {\r\n                    name: 'John Doe',\r\n                    books: [\r\n                        'Vue 2 - Advanced Guide',\r\n                        'Vue 3 - Basic Guide',\r\n                        'Vue4 -The Mystery'\r\n                    ]\r\n                },\r\n                firstName: 'John',\r\n                lastName: 'Doe'\r\n            }\r\n        },\r\n        computed: {\r\n            // 一个计算属性的 getter\r\n            publishedBooksMessage() {\r\n                // this 指向当前组件实例\r\n                return this.author.books.length > 0 ? 'yes' : 'NO'\r\n            },\r\n            fullName: {\r\n                get() {\r\n                    return this.firstName + ' ' + this.lastName\r\n                },\r\n                set(newValue) {\r\n                    [this.firstName, this.lastName] = newValue.split('')\r\n                }\r\n            }\r\n        },\r\n        setup() {\r\n            const objectOfAttrs = {\r\n                id: 'container',\r\n                class: 'warpper'\r\n            };\r\n            return { \r\n                \r\n                name: \"my Ye\", \r\n                \r\n                dynamicId: 1,\r\n                \r\n                isButtonDisabled: true,\r\n                \r\n                objectOfAttrs,\r\n                url: 'https://router.vuejs.org',\r\n\r\n                container: 92323\r\n            }\r\n        },\r\n        methods: {\r\n            async increment() {\r\n                this.count++\r\n                await nextTick()\r\n                // 现在 DOM 已经更新了\r\n            },\r\n            mutateDeeply() {\r\n                // 以下都会按照期望工作\r\n                this.obj.nested.count++\r\n                this.obj.arr.push('baz')\r\n                this.obj.arr.push('bar')\r\n            }\r\n        },\r\n        mounted() {\r\n            // `this` 指向当前组件实例\r\n            console.log(this.count) // => 1\r\n\r\n            // 数据属性也可以被更改\r\n            this.increment()\r\n            this.mutateDeeply()\r\n            console.log(this.obj)\r\n            const newObject = {}\r\n            this.someObject = newObject\r\n\r\n            console.log(this.newObject === this.someObject) // false\r\n        }\r\n    }\r\n</script>\r\n<style scoped>\r\n    h3 {\r\n        margin: 40px 0 0;\r\n    }\r\n\r\n    ul {\r\n        list-style-type: none;\r\n        padding: 0;\r\n    }\r\n\r\n    li {\r\n        display: inline-block;\r\n        margin: 0 10px;\r\n    }\r\n\r\n    .warpper {\r\n        width: 200px;\r\n        height: 200px;\r\n        background: black;\r\n        color: darkred;\r\n    }\r\n</style>"],"mappings":";AAmBI,SAASA,QAAO,QAAS,KAAI;AAC7B,eAAe