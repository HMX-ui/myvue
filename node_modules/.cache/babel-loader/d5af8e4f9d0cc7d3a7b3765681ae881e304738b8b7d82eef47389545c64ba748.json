sole.log(this.count) // => 1\r\n\r\n            // 数据属性也可以被更改\r\n            this.increment()\r\n            this.mutateDeeply()\r\n            console.log(this.obj)\r\n            const newObject = {}\r\n            this.someObject = newObject\r\n\r\n            console.log(this.newObject === this.someObject) // false\r\n        }\r\n    }\r\n</script>\r\n<style scoped>\r\n    h3 {\r\n        margin: 40px 0 0;\r\n    }\r\n\r\n    ul {\r\n        list-style-type: none;\r\n        padding: 0;\r\n    }\r\n\r\n    li {\r\n        display: inline-block;\r\n        margin: 0 10px;\r\n    }\r\n\r\n    .warpper {\r\n        width: 200px;\r\n        height: 200px;\r\n        background: black;\r\n        color: darkred;\r\n    }\r\n</style>"],"mappings":";;;EACSA,KAAK,EAAC;AAAO;mBADtB;mBAAA;mBAAA;mBAAA;gEAcIC,mBAAA,CAA2B,WAAxB,sBAAoB;;uBAd3BC,mBAAA,CAAAC,SAAA,SACIF,mBAAA,CAMM,OANNG,UAMM,GALFH,mBAAA,CAA0C;IAAtCI,IAAI,EAAEC,MAAA,CAAAC;EAAG,GAAE,yBAAuB,iBAF9CC,UAAA,GAGQP,mBAAA,CAAwB,YAAAQ,gBAAA,CAAjBH,MAAA,CAAAI,IAAI,IAAG,OAAK,iBACnBT,mBAAA,CAAoB,YAAAQ,gBAAA,CAAbE,KAAA,CAAAC,KAAK,kBACZX,mBAAA,CAA6B,YAAAQ,gBAAA,CAAtBE,KAAA,CAAAE,GAAG,CAACC,GAAG,CAACC,MAAM,kBACrBd,mBAAA,CAA+B,YAAAQ,gBAAA,CAAxBE,KAAA,CAAAE,GAAG,CAACG,MAAM,CAACJ,KAAK,iB,GAE3BX,mBAAA,CAA+C;IAAtCgB,OAAK,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEC,QAAA,CAAAC,SAAA,IAAAD,QAAA,CAAAC,SAAA,IAAAF,IAAA,CAAS;sBAAKR,KAAA,CAAAC,KAAK,kBACnCX,mBAAA,CAA+C;IAAnCqB,EAAE,EAAEhB,MAAA,CAAAiB;EAAS,GAAE,gBAAc,iBAT7CC,UAAA,GAUIvB,mBAAA,CAAmD;IAA7CqB,EAAE,UAAUhB,MAAA,CAAAiB,SAAS;KAAI,gBAAc,iBAVjDE,UAAA,GAWIxB,mBAAA,CAAgD;IAAvCyB,QAAQ,EAAEpB,MAAA,CAAAqB;EAAgB,GAAE,IAAE,iBAX3CC,UAAA,GAYI3B,mBAAA,CAAiD,OAZrD4B,eAAA,CAAAC,mBAAA,CAYiBxB,MAAA,CAAAyB,aAAa,IAAE,iBAAe,wBAC3C9B,mBAAA,CAAoD,aAA/C,oBAAkB,GAAAQ,gBAAA,CAAGH,MAAA,CAAAyB,aAAa,CAACT,EAAE,kBAC1CU,UAA2B,EAC3B/B,mBAAA,CAAwC,cAAAQ,gBAAA,CAA/BW,QAAA,CAAAa,qBAAqB,iB","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}